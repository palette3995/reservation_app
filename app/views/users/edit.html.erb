

<div class="links">
  <p class=" account no-active"><%= link_to "アカウント", edit_user_registration_path %></p>
  <p class=" plofile active"><%= link_to "プロフィール", edit_user_path(current_user.id) %></p>
</div>

     <h1>プロフィール</h1>

     <%= form_with model: @user do |f| %>
      <table class="table">
        <tr>
          <th><%= f.label :avatar ,"アイコン画像" %></th>
          <td><%= f.file_field :avatar %></td>
        </tr>
        <tr>
          <th><%= f.label :name ,"名前" %></th>
          <td><%= f.text_field :name %></td>
        </tr>
        <tr>
          <th><%= f.label :profile ,"自己紹介" %></th>
          <td><%= f.text_area :profile %></td>
        </tr>
      </table>
      <div>
        <ul>
          <li><%= f.submit "更新"%></li>
        </ul>
      </div>
     <% end %>
